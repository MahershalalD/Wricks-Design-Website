        <div class="container">
    <div class="card car"    style="margin-bottom: 50px !important">
        <div class="card-header">
            <div class="row">
                <div class="col-lg-6" style="font-family: 'Anton', sans-serif;">
                   <h3>Orders</h3> 
                </div>
                <div class="col-lg-6" >
                    <span class="blinking">ðŸŸ¢</span> In Progress
                </div>
            </div>
        </div>
            <% vaar.forEach(function(found){ %>
                <% if(found.completed!=1){ %>
<div class="card-body">
    <table>
        <tr>
            <th>
                <h5 style="font-family: 'Coda Caption', sans-serif;">Work Id: </h5>
            </th>
            <th>
                <h5 style="font-family: 'PT Sans', sans-serif;"> <%= found.orderId %></h5>
            </th>
        </tr>
        <tr>
            <th>
                <h5 style="font-family: 'Coda Caption', sans-serif;">Work: </h5>
            </th>
            <th>
                <h5 style="font-family: 'PT Sans', sans-serif;"> <%= found.typeOfWork %></h5>
            </th>
        </tr>
        <tr>
            <th>
                <h5 style="font-family: 'Coda Caption', sans-serif;">Expected Date of Delivery: </h5>
            </th>
            <th>
                <h5 style="font-family: 'PT Sans', sans-serif;"> <%= found.Duedate %></h5>
            </th>
        </tr>
    </table>

    <h5>Work Progress : </h5>
    <div class="progress">
        <% if(found.comper===10){ %>
        <div class="progress-bar progress-bar-striped progress-bar-animated bg-danger" role="progressbar"
            aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 10%"></div>
        <% } %>
        <% if(found.comper===20){ %>
        <div class="progress-bar progress-bar-striped progress-bar-animated bg-danger" role="progressbar"
            aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 20%"></div>
        <% } %>
        <% if(found.comper===30){ %>
        <div class="progress-bar progress-bar-striped progress-bar-animated bg-danger" role="progressbar"
            aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 30%"></div>
        <% } %>
        <% if(found.comper===40){ %>
        <div class="progress-bar progress-bar-striped progress-bar-animated bg-danger" role="progressbar"
            aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 40%"></div>
        <% } %>
        <% if(found.comper===50){ %>
        <div class="progress-bar progress-bar-striped progress-bar-animated bg-danger" role="progressbar"
            aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 50%"></div>
        <% } %>
        <% if(found.comper===60){ %>
        <div class="progress-bar progress-bar-striped progress-bar-animated bg-danger" role="progressbar"
            aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 60%"></div>
        <% } %>
        <% if(found.comper===70){ %>
        <div class="progress-bar progress-bar-striped progress-bar-animated bg-danger" role="progressbar"
            aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 70%"></div>
        <% } %>
        <% if(found.comper===80){ %>
        <div class="progress-bar progress-bar-striped progress-bar-animated bg-danger" role="progressbar"
            aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 80%"></div>
        <% } %>
        <% if(found.comper===90){ %>
        <div class="progress-bar progress-bar-striped progress-bar-animated bg-danger" role="progressbar"
            aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 90%"></div>
        <% } %>
        <% if(found.comper===100){ %>
        <div class="progress-bar progress-bar-striped progress-bar-animated bg-danger" role="progressbar"
            aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 100%"></div>

        <% } %>
    </div>
</div>
<%  if(found.Feedback===1){ %>
<div class="card text-center" style="margin-bottom: 100px !important">
    <div class="card-header">
        Feedback
    </div>
    <div class="card-body">

        <form action="/freply" method="POST">
            <h5 class="card-title">Give your stars</h5>
            <span class="fa fa-star a"></span>
            <span class="fa fa-star b"></span>
            <span class="fa fa-star c"></span>
            <span class="fa fa-star d"></span>
            <span class="fa fa-star e"></span>
            <h5>Give Your Feedback</h5>
            <input type="hidden" value="<%= found._id %> " name="fed1">
            <input type="hidden" value="<%= found.orderId %>" name="kkk">
            <div class="form-group">
                <textarea class="form-control" name="fees" id="exampleFormControlTextarea1" rows="3"
                    required></textarea>
            </div>
            <button class="btn-lo" type="submit" name="kk" value=" ">Submit</button>
        </form>

    </div>
</div>

<% } %>               
    <% } %>
         <% }) %>  
    
